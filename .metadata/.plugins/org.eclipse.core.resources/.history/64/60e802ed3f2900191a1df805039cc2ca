package com.service.patient.cloud.property;

import org.springframework.beans.factory.annotation.Value;

public class DataSourcePropertyHolder {
	
	@Value("${patients.datasource.type}")
	private String type;
	
	@Value("${patients.datasource.nae}")
	private String name;
	
	@Value("${patients.datasource.port}")
	private int port;
	
	@Value("${patients.datasource.host}")
	private String host;
	
	@Value("${patients.datasource.root}")
	private String username;
	
	@Value("${patients.datasource.root}")
	private String password;
	
	@Value("${patients.datasource.uri}")
	
	private String uri;
	
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPort() {
		return port;
	}
	public void setPort(int port) {
		this.port = port;
	}
	public String getHost() {
		return host;
	}
	public void setHost(String host) {
		this.host = host;
	}
	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
		this.username = username;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public String getUri() {
		return uri;
	}
	public void setUri(String uri) {
		this.uri = uri;
	}
}
